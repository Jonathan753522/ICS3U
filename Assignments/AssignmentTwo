import math

#Functions
def PhotoFactors(N):
    Photonum = []
    mnum = math.floor(math.sqrt(N))
    for Xaxis in range(1,mnum + 1): 
        if N % Xaxis == 0:  
            Photonum.append(Xaxis)
  
    return Photonum

def perim(N):
    Factor = PhotoFactors(N)
    Xaxis = 1
    for num in Factor:
        if num > Xaxis:
            Xaxis = num 
    Yaxis = N / Xaxis
    perimeter = 2 * (Xaxis + Yaxis)
    print("Minimum perimeter is %d with dimensions: %d x %d" % (perimeter, Xaxis, Yaxis))

#Printing Statments
print("Welcome to the school yearbook program!")
userinp = int(input("Please Input A Number Of Photographs: "))
photooutput = userinp

if photooutput <= 0:
    print("Please input a positive number")

elif photooutput > 0:
    perim(photooutput)
